<odoo>
    <data>
        <record model="ir.ui.view" id="udf_account_analytic_account_inherit_form">
            <field name="name">udf.account.analytic.account.inherit.form</field>
            <field name="model">account.analytic.account</field>
            <field name="inherit_id" ref="analytic.view_account_analytic_account_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='plan_id']" position="after">
                    <field name="plan_name" invisible="1"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>